{
  "name": "technical_assistance_portal",
  "version": "1.0.0",
  "description": "technical assistance portal under development",
  "main": "server.js",
  "license": "MIT",
  "scripts": {
    "build": "babel src --extensions \".ts\" --out-dir build --copy-files --no-copy-ignored",
    "commit": "gitmoji -c",
    "dev": "ts-node-dev -r --respawn --transpile-only --ignore-watch node_modules src/server.ts",
    "prisma:deploy": "npx prisma migrate deploy",
    "prisma:migrate": "npx prisma migrate dev --name",
    "prisma:reset": "npx prisma migrate reset",
    "prisma:seed": "prisma db push && node prisma/seeds/index.ts",
    "prisma:studio": "npx prisma studio"
  },
  "devDependencies": {
    "@types/node": "^20.1.7",
    "prisma": "^4.14.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@prisma/client": "^4.14.0",
    "@types/express": "^4.17.17",
    "express": "^4.18.2"
  }
}
